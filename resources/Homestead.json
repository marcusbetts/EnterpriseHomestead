{
    "ip": "192.168.20.20",
    "memory": 2048,
    "cpus": 1,
    "provider": "virtualbox",
    "authorize": "~/.ssh/id_rsa.pub",
    "keys": [
        "~/.ssh/id_rsa"
    ],
    "folders": [
        {
            "map": "~/Code",
            "to": "/home/vagrant/Code"
        }
    ],
    "sites": [
        {
            "map": "homestead.app",
            "to": "/home/vagrant/Code/public"
        }
    ],
    "databases": [
        "homestead"
    ]
}
